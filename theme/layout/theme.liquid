<main id="main" role="main" data-router-wrapper>
  {% if template.name == "index" %}
    {% assign namespace = "home" %}
  {% elsif template.suffix == 'gummy-trial' %}
    {% assign namespace = "product-trial" %}
  {% elsif template.name == "page" %}
    {% if template.suffix %}
      {% assign namespace = template.suffix %}
    {% else %}
      {% assign namespace = template.name %}
    {% endif %}
  {% else %}
    {% assign namespace = template.name %}
  {% endif %}
  <div class="view-{{ namespace }}" data-router-view="{{ namespace }}">
    {{ content_for_layout }}
  </div>
</main>
