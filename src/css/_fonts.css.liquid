/*
	Store your @font-face in here
-------------------------------------------------- */
@font-face {
	font-family: 'NeueMontreal-Regular';
	src: url({{ "NeueMontreal-Regular.woff2" | asset_url }}) format("woff2");
	font-weight: normal;
	font-style: normal;
}

@font-face {
	font-family: 'NeueMontreal-Medium';
	src: url({{ "NeueMontreal-Medium.woff2" | asset_url }}) format("woff2");
	font-weight: normal;
	font-style: normal;
}


